package kafka.consumer;

import org.springframework.kafka.annotation.KafkaListener;

public class SignUpConsumer {
	
	@Autowired
	private UserRepository repository;
	
	
	@KafkaListener(topics = "login", containerFactory = "userKafkaListenerContainerFactory")
    public boolean signUpListener(String str) {
		try {
			System.out.println("TestConsumer End");
			return true;
		}catch(Exception e) {
			e.printStackTrace();
			System.out.println("TestConsumer Error");
			return false;
		}
    }
}
